{
  "name": "@fokke-/vue-bartender.js",
  "version": "1.0.0",
  "description": "Vue components for Bartender.js",
  "author": "Ville Fokke Saarivaara",
  "license": "MIT",
  "private": false,
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/Fokke-/vue-bartender.js"
  },
  "keywords": [
    "off-canvas",
    "accessibility",
    "a11y",
    "sidebar",
    "navigation"
  ],
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint ./src",
    "format": "prettier --write src/"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist"
  ],
  "types": "./dist/index.d.ts",
  "module": "./dist/VueBartender.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/VueBartender.js"
    }
  },
  "dependencies": {
    "@fokke-/bartender.js": "3.0.0-beta.4"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.10.4",
    "@tsconfig/node20": "20.1.4",
    "@types/node": "22.7.6",
    "@typescript-eslint/eslint-plugin": "8.9.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vue/eslint-config-prettier": "10.0.0",
    "@vue/eslint-config-typescript": "14.1.1",
    "@vue/tsconfig": "0.5.1",
    "@vue/typescript-plugin": "2.1.6",
    "eslint": "9.12.0",
    "modern-normalize": "3.0.1",
    "prettier": "3.3.3",
    "sass": "1.80.2",
    "typescript": "5.4.2",
    "typescript-eslint": "8.10.0",
    "vite": "5.4.9",
    "vite-plugin-dts": "4.2.4",
    "vite-plugin-eslint": "1.8.1",
    "vue": "3.5.12",
    "vue-router": "4.4.5",
    "vue-tsc": "2.1.6"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}
